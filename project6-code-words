#conversion to upper case letter + reassign to list 
def letter_coder():
  for i in range(len(num_list)):
    letter = chr(num_list[i] + 64)
    letter_list.append(letter)
    
#turn string into list of integers
numbers = input("Enter a series of numbers you wish to convert to their corresponding letters of the alphabet (separate each number with a space): ")
temp_list = numbers.split()

num_list = []
letter_list = []

for i in range(len(temp_list)):
  num_list.append(int(temp_list[i]))
  
#run subroutine  
letter_coder()
print(letter_list)  
